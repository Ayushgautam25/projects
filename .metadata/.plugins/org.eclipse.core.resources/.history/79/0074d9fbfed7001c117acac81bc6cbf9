package vehicle;

public class VehicleProperties {
	private String Engine;
	private int gear;
	private Fuel FuelSystem;
	public VehicleProperties() {
		super();
		// TODO Auto-generated constructor stub
	}
	
	@Override
	public String toString() {
		return "VehicleProperties [Engine=" + Engine + ", gear=" + gear + ", f=" + FuelSystem + "]";
	}

	public String getEngine() {
		return Engine;
	}

	public void setEngine(String engine) {
		Engine = engine;
	}

	public int getGear() {
		return gear;
	}

	public void setGear(int gear) {
		this.gear = gear;
	}

	public Fuel getFuelSystem() {
		return FuelSystem;
	}

	public void setFuelSystem(Fuel FuelSystem) {
		this.FuelSystem = FuelSystem;
	}

	public VehicleProperties(String engine, int gear, Fuel FuelSystem) {
		super();
		Engine = engine;
		this.gear = gear;
		this.FuelSystem = FuelSystem;
	}

	public void inititit() {
		System.out.println("Information of car :");
	}
	public void byebye() {
		System.out.println("Bye Bye!");
	}
}
